	<queryString>
		<![CDATA[SELECT 1 as pag, ss.* FROM ( SELECT dp.id, r.numero, r.importe, r.fecha_nota_credito, r.observacion, dp.cantidad, dp.precio_unitario, iva.iva, con.nombre as cliecontrib,
